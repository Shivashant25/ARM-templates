{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "apiProfile": "2020-09-01-hybrid",
  "parameters": {
    "DeploymentID": {
      "type": "string"
    }
  },
  "variables": {
    "SubID": "[subscription().subscriptionId]",
    "Webappname": "[concat('fabmedical-web-',parameters('DeploymentID'))]",
    "Appserviceplanname": "[concat('fabmedical-plan-',parameters('DeploymentID'))]",
    "Cosmosdbname": "[concat('fabmedical-cdb-',parameters('DeploymentID'))]",
    "publishingUsername": "[concat('$',parameters('DeploymentID'))]",
    "location": "[resourceGroup().location]",
    "loadtestname": "[concat('fabmedical-loadtest-',parameters('DeploymentID'))]",
    "projectname": "[concat('devproject-',parameters('DeploymentID'))]",
    "rgname": "[resourceGroup().name]",
    "devCenterId": "[resourceId(resourceGroup().name,'Microsoft.DevCenter/devcenters',variables('devcentername'))]",
    "devcentername": "[concat('devcenter-',parameters('DeploymentID'))]",
    "devboxvnet": "devboxvnet",
    "devboxsubnet": "devboxsubnet",
    "subnetPrefix": "10.1.0.0/24",
    "addressPrefix": "10.1.0.0/16",
    "netconnectionname": "fabrikam-connection",
    "vnetId1": "[resourceId(resourceGroup().name,'Microsoft.Network/virtualNetworks', variables('devboxvnet'))]",
    "devboxsubnetid": "[concat(variables('vnetId1'), '/subnets/', variables('devboxsubnet'))]"
  },
  "resources": [
    {
      "type": "Microsoft.DocumentDB/databaseAccounts",
       "name": "[variables('Cosmosdbname')]",
      "location": "[variables('location')]",
      "tags": {
      },
      "kind": "MongoDB",
      "identity": {
        "type": "None"
      },
      "properties": {
        "publicNetworkAccess": "Enabled",
        "enableAutomaticFailover": false,
        "enableMultipleWriteLocations": true,
        "isVirtualNetworkFilterEnabled": false,
        "virtualNetworkRules": [],
        "disableKeyBasedMetadataWriteAccess": false,
        "enableFreeTier": false,
        "enableAnalyticalStorage": false,
        "analyticalStorageConfiguration": {
          "schemaType": "FullFidelity"
        },
        "databaseAccountOfferType": "Standard",
        "defaultIdentity": "FirstPartyIdentity",
        "networkAclBypass": "None",
        "disableLocalAuth": false,
        "consistencyPolicy": {
          "defaultConsistencyLevel": "Session",
          "maxIntervalInSeconds": 5,
          "maxStalenessPrefix": 100
        },
        "apiProperties": {
          "serverVersion": "3.6"
        },
        "locations": [
          {
            "locationName": "[variables('location')]",
            "provisioningState": "Succeeded",
            "failoverPriority": 0,
            "isZoneRedundant": false
          },
          {
            "locationName": "West US2",
            "provisioningState": "Succeeded",
            "failoverPriority": 1,
            "isZoneRedundant": true
          }
        ],
        "cors": [],
        "capabilities": [
          {
            "name": "EnableMongo"
          }
        ],
        "ipRules": [],
        "backupPolicy": {
          "type": "Periodic",
          "periodicModeProperties": {
            "backupIntervalInMinutes": 240,
            "backupRetentionIntervalInHours": 8,
            "backupStorageRedundancy": "Geo"
          }
        },
        "networkAclBypassResourceIds": [],
        "diagnosticLogSettings": {
          "enableFullTextQuery": "None"
        }
      }
    },
    {
      "type": "Microsoft.Web/serverfarms",
      "name": "[variables('Appserviceplanname')]",
      "location": "[variables('location')]",
      "tags": {
      },
      "sku": {
        "name": "S1",
        "tier": "Standard",
        "size": "S1",
        "family": "S",
        "capacity": 1
      },
      "kind": "linux",
      "properties": {
        "perSiteScaling": false,
        "elasticScaleEnabled": false,
        "maximumElasticWorkerCount": 1,
        "isSpot": false,
        "reserved": true,
        "isXenon": false,
        "hyperV": false,
        "targetWorkerCount": 0,
        "targetWorkerSizeId": 0,
        "zoneRedundant": false
      }
    },
    {
      "type": "Microsoft.Web/sites",
      "name": "[variables('Webappname')]",
      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms', variables('Appserviceplanname'))]"
      ],
      "tags": {
      },
      "kind": "app,linux,container",
      "properties": {
        "enabled": true,
        "hostNameSslStates": [
          {
            "name": "[concat(variables('Webappname'), '.azurewebsites.net')]",
            "sslState": "Disabled",
            "hostType": "Standard"
          },
          {
            "name": "[concat(variables('Webappname'), '.scm.azurewebsites.net')]",
            "sslState": "Disabled",
            "hostType": "Repository"
          }
        ],
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', variables('Appserviceplanname'))]",
        "reserved": true,
        "isXenon": false,
        "hyperV": false,
        "siteConfig": {
          "numberOfWorkers": 1,
          "linuxFxVersion": "DOCKER|nginx",
          "acrUseManagedIdentityCreds": false,
          "alwaysOn": true,
          "http20Enabled": true,
          "functionAppScaleLimit": 0,
          "minimumElasticInstanceCount": 0
        },
        "scmSiteAlsoStopped": false,
        "clientAffinityEnabled": true,
        "clientCertEnabled": false,
        "clientCertMode": "Required",
        "hostNamesDisabled": false,
        "customDomainVerificationId": "DC7D747619D7E060DE1F3059D1B6C53F698897C78668B44D90CF7B193EEC720B",
        "containerSize": 0,
        "dailyMemoryTimeQuota": 0,
        "httpsOnly": false,
        "redundancyMode": "None",
        "storageAccountRequired": false,
        "keyVaultReferenceIdentity": "SystemAssigned"
      }
    },
    {
      "type": "Microsoft.Web/sites/basicPublishingCredentialsPolicies",
      "name": "[concat(variables('Webappname'), '/ftp')]",
      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Web/sites', variables('Webappname'))]"
      ],
      "tags": {
      },
      "properties": {
        "allow": true
      }
    },
    {
      "type": "Microsoft.Web/sites/basicPublishingCredentialsPolicies",
      "apiVersion": "2021-03-01",
      "name": "[concat(variables('Webappname'), '/scm')]",
      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Web/sites', variables('Webappname'))]"
      ],
      "tags": {
      },
      "properties": {
        "allow": true
      }
    },
    {
      "type": "Microsoft.Web/sites/config",
      "name": "[concat(variables('Webappname'), '/web')]",
      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Web/sites', variables('Webappname'))]"
      ],
      "tags": {
      },
      "properties": {
        "numberOfWorkers": 1,
        "defaultDocuments": [
          "Default.htm",
          "Default.html",
          "Default.asp",
          "index.htm",
          "index.html",
          "iisstart.htm",
          "default.aspx",
          "index.php",
          "hostingstart.html"
        ],
        "netFrameworkVersion": "v4.0",
        "linuxFxVersion": "DOCKER|nginx",
        "requestTracingEnabled": false,
        "remoteDebuggingEnabled": false,
        "remoteDebuggingVersion": "VS2019",
        "httpLoggingEnabled": false,
        "acrUseManagedIdentityCreds": false,
        "logsDirectorySizeLimit": 35,
        "detailedErrorLoggingEnabled": false,
        "publishingUsername": "[variables('publishingUsername')]",
        "scmType": "None",
        "use32BitWorkerProcess": true,
        "webSocketsEnabled": false,
        "alwaysOn": true,
        "managedPipelineMode": "Integrated",
        "virtualApplications": [
          {
            "virtualPath": "/",
            "physicalPath": "site\\wwwroot",
            "preloadEnabled": true
          }
        ],
        "loadBalancing": "LeastRequests",
        "experiments": {
          "rampUpRules": []
        },
        "autoHealEnabled": false,
        "vnetRouteAllEnabled": false,
        "vnetPrivatePortsCount": 0,
        "localMySqlEnabled": false,
        "ipSecurityRestrictions": [
          {
            "ipAddress": "Any",
            "action": "Allow",
            "priority": 1,
            "name": "Allow all",
            "description": "Allow all access"
          }
        ],
        "scmIpSecurityRestrictions": [
          {
            "ipAddress": "Any",
            "action": "Allow",
            "priority": 1,
            "name": "Allow all",
            "description": "Allow all access"
          }
        ],
        "scmIpSecurityRestrictionsUseMain": false,
        "http20Enabled": true,
        "minTlsVersion": "1.2",
        "scmMinTlsVersion": "1.2",
        "ftpsState": "AllAllowed",
        "preWarmedInstanceCount": 0,
        "functionAppScaleLimit": 0,
        "functionsRuntimeScaleMonitoringEnabled": false,
        "minimumElasticInstanceCount": 0,
        "azureStorageAccounts": {}
      }
    },
    {
      "type": "Microsoft.Web/sites/hostNameBindings",
      "name": "[concat(variables('Webappname'), '/', variables('Webappname'), '.azurewebsites.net')]",
      "location": "[variables('location')]",
      "dependsOn": [
        "[resourceId('Microsoft.Web/sites', variables('Webappname'))]"
      ],
      "properties": {
        "siteName": "[variables('Webappname')]",
        "hostNameType": "Verified"
      }
    },
    {
      "type": "Microsoft.LoadTestService/loadtests",
      "apiVersion": "2022-04-15-preview",
      "name": "[variables('loadtestname')]",
      "location": "[variables('location')]"
    },
    {
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[variables('devboxvnet')]",
      "location": "[variables('location')]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('addressPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('devboxsubnet')]",
            "properties": {
              "addressPrefix": "[variables('subnetPrefix')]"
            }
          }
        ]
      }
    },
    {
      "type": "Microsoft.DevCenter/devcenters",
      "name": "[variables('devcentername')]",
      "location": "[variables('location')]",
      "identity": {
        "type": "none"
      }
    },
    {
      "type": "Microsoft.DevCenter/networkconnections",
      "name": "[variables('netconnectionname')]",
      "dependsOn": [
        "[concat('Microsoft.Network/virtualNetworks/', variables('devboxvnet'))]"
      ],
      "location": "[variables('location')]",

      "properties": {
        "domainJoinType": "AzureADJoin",
        "subnetId": "[variables('devboxsubnetid')]"
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "name": "fabrikamdeploy-project",
      "dependsOn": [
        "[concat('Microsoft.DevCenter/devcenters/', variables('devcentername'))]"
      ],
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "apiProfile": "2020-09-01-hybrid",
          "parameters": {},
          "variables": {
            "location": "[variables('location')]",
            "projectname": "[variables('projectname')]",
            "devCenterId": "[variables('devCenterId')]",
            "subscriptionId": "[variables('SubID')]",
            "resourceGroup": "[variables('rgname')]"
          },
          "resources": [
            {
              "name": "[variables('projectname')]",
              "location": "[variables('location')]",
              "type": "Microsoft.DevCenter/projects",
              "properties": {
                "devCenterId": "[variables('devCenterId')]"
              }
            }
          ]

        }
      },
      "subscriptionId": "[variables('SubID')]",
      "resourceGroup": "[variables('rgname')]"
    }
  ]
}
